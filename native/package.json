{
  "name": "native",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "native",
    "triples": {
      "additional": [
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.16.3",
    "ava": "^5.1.1",
    "cpy-cli": "^5.0.0",
    "make-dir-cli": "^3.1.0"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release && pnpm build:out",
    "build:win": "napi build --platform --release --target x86_64-pc-windows-msvc && pnpm build:out",
    "build:debug": "napi build --platform",
    "build:out": "make-dir dist && cpy package.json dist/ && cpy index.d.ts dist/ && cpy index.js dist/ && cpy *.node dist/",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version"
  }
}